{
  "model": {
    "type": "minecraft:select",
    "property": "minecraft:custom_model_data",
    "cases": [
      {
        "when": "body",
        "model": {
          "type": "minecraft:condition",
          "property": "minecraft:custom_model_data",
          "on_true": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:custom_model_data",
            "index": 0,
            "entries": [
              {
                "threshold": 1,
                "model": {
                  "type": "minecraft:composite",
                  "models": [
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/hurt/body"
                    },
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/hurt/legs/walk"
                    }
                  ]
                }
              },
              {
                "threshold": 2,
                "model": {
                  "type": "minecraft:composite",
                  "models": [
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/hurt/body"
                    },
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/hurt/legs/crawl"
                    }
                  ]
                }
              }
            ],  
            "fallback": {
              "type": "minecraft:composite",
              "models": [
                {
                  "type": "minecraft:model",
                  "model": "magical_addons:entity/sculk_spider/hurt/body"
                },
                {
                  "type": "minecraft:model",
                  "model": "magical_addons:entity/sculk_spider/hurt/legs/idle"
                }
              ]
            }
          },
          "on_false": {
            "type": "minecraft:range_dispatch",
            "property": "minecraft:custom_model_data",
            "index": 0,
            "entries": [
              {
                "threshold": 1,
                "model": {
                  "type": "minecraft:composite",
                  "models": [
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/normal/body"
                    },
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/normal/legs/walk"
                    }
                  ]
                }
              },
              {
                "threshold": 2,
                "model": {
                  "type": "minecraft:composite",
                  "models": [
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/normal/body"
                    },
                    {
                      "type": "minecraft:model",
                      "model": "magical_addons:entity/sculk_spider/normal/legs/crawl"
                    }
                  ]
                }
              }
            ],  
            "fallback": {
              "type": "minecraft:composite",
              "models": [
                {
                  "type": "minecraft:model",
                  "model": "magical_addons:entity/sculk_spider/normal/body"
                },
                {
                  "type": "minecraft:model",
                  "model": "magical_addons:entity/sculk_spider/normal/legs/idle"
                }
              ]
            }
          }
        }
      }
    ],
    "fallback": {
      "type": "minecraft:condition",
      "property": "minecraft:custom_model_data",
      "on_true": {
        "type": "minecraft:model",
        "model": "magical_addons:entity/sculk_spider/hurt/head"
      },
      "on_false": {
        "type": "minecraft:model",
        "model": "magical_addons:entity/sculk_spider/normal/head"
      }
    }
  }
}